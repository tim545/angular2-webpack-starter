{
  "name": "angular2-webpack-starter",
  "version": "1.0.0",
  "description": "A webpack starter for Angular2",
  "scripts": {
    "start": "rimraf dist && grunt && webpack-dev-server --inline --progress --port 4200 --open --content-base dist",
    "test": "karma start",
    "dev": "rimraf dist && grunt && webpack --config config/webpack.dev.js --progress --profile --bail",
    "staging": "rimraf dist && grunt && webpack --config config/webpack.staging.js --progress --profile --bail",
    "prod": "rimraf dist && grunt && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install",
    "i18n": "ng-xi18n"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "^0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "^2.0.1",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "ng2-bootstrap": "^1.1.5",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "^1.0.0-beta.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-sass": "^1.2.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "load-grunt-tasks": "^3.5.2",
    "node-sass": "^3.10.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "pug-html-loader": "^1.0.8",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "time-grunt": "^1.4.0",
    "to-string-loader": "^1.1.5",
    "tslint": "^3.15.1",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
